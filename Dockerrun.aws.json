{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "686255986323.dkr.ecr.us-east-1.amazonaws.com/seven-cells-backend:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000
        }
      ]
    },
    {
      "name": "frontend",
      "image": "686255986323.dkr.ecr.us-east-1.amazonaws.com/seven-cells-frontend:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000
        }
      ]
    }
  ]
}
